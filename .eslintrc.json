{
  "extends": ["next/core-web-vitals"],
  "rules": {
    "semi": ["error", "always"],
    "quotes": ["error", "double"],
    "no-unused-vars": ["error", { "vars": "all", "args": "none" }],
    "react/prop-types": "off",
    "no-console": "warn"
  },
  "plugins": ["react"],
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jest": true
  },
  "parserOptions": {
    "parser": "babel-eslint",
    "sourceType": "module",
    "ecmaVersion": "latest",
    "allowImportExportEverywhere": true
  }
}
